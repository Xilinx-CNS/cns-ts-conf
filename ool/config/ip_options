# This option provides inserting of 4 IP options: NOP NOP NOP EOP
# to TCP/UDP packets which are sent by Agt_B using
# interfaces TE_TST1_IUT and TE_TST1_IUT_IUT
export SOCKAPI_TS_IP_OPTIONS=1
TE_EXTRA_OPTS="$TE_EXTRA_OPTS --trc-tag=ip_options"
