# Onload does not use huge pages for packet buffers (which is default), but
# uses huge-sized large compound pages.
export EF_USE_HUGE_PAGES=0
TE_EXTRA_OPTS="--trc-tag=pkt_nohuge --trc-tag=pkt_largecomp"
