# Transform IUT SoC into IUT
export TE_IUT=${TE_IUT_SOC}
unset TE_IUT_SOC
export TE_IUT_SSH_USER=${TE_IUT_SOC_SSH_USER}
unset TE_IUT_SOC_SSH_USER
export TE_IUT_REBOOTABLE=${TE_IUT_SOC_REBOOTABLE}
unset TE_IUT_SOC_REBOOTABLE
export TE_IUT_MEM_CHANNELS=${TE_IUT_SOC_MEM_CHANNELS}
unset TE_IUT_SOC_MEM_CHANNELS
export TE_IUT_MEM_AMOUNT=${TE_IUT_SOC_MEM_AMOUNT}
unset TE_IUT_SOC_MEM_AMOUNT

export TE_PCI_VENDOR_IUT_TST1=${TE_PCI_VENDOR_IUT_SOC}
unset TE_PCI_VENDOR_IUT_SOC
export TE_PCI_DEVICE_IUT_TST1=${TE_PCI_DEVICE_IUT_SOC}
unset TE_PCI_DEVICE_IUT_SOC
export TE_PCI_INSTANCE_IUT_TST1=${TE_PCI_INSTANCE_IUT_SOC:-0}
unset TE_PCI_INSTANCE_IUT_SOC
export TE_ENV_IUT_DPDK_DRIVER="${TE_ENV_IUT_SOC_DPDK_DRIVER}"
unset TE_ENV_IUT_SOC_DPDK_DRIVER
export TE_ENV_IUT_NET_DRIVER=${TE_ENV_IUT_SOC_NET_DRIVER}
unset TE_ENV_IUT_SOC_NET_DRIVER
export TE_IUT_REQUIRED_SERVICE_CORES=${TE_IUT_SOC_REQUIRED_SERVICE_CORES}
unset TE_IUT_SOC_REQUIRED_SERVICE_CORES

export TE_IUT_DPDK_CFLAGS="-DRTE_PMD_NET_SFC_NIC_DMA_MAP"
